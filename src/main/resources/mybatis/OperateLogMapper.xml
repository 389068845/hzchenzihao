<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.netease.dao.OperateLogDao">
	<!-- 新增记录 -->
	<insert id="insert" parameterType="com.netease.dao.po.OperateLog" >
		insert into operate_log values(#{userId},#{operate})
	</insert>
	<!-- 查询单条记录 -->
	<select id="selectByAccount" parameterType="java.lang.String" resultType="com.netease.dao.po.OperateLog">
		select * from operate_log where user_id = #{userId}
	</select>

</mapper> 